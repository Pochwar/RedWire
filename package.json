  "description": "Le site qu'il est bien pour les s√©ries",
  "main": "src/app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "nodemon": "nodemon ./src/app.js",
    "faker": "node ./src/faker/index.js",
    "serve:html": "lite-server --baseDir=html-examples",
    "lint": "eslint  ./src/**/*.js || (exit 0)",
    "test": "mocha --recursive || (exit 0)",
    "test:watch": "mocha -w --recursive || (exit 0)",
    "test:public": "mocha --recursive --grep Public || (exit 0)",
    "test:member": "mocha --recursive --grep Member || (exit 0)",
    "test:admin": "mocha --recursive --grep Admin || (exit 0)",
    "test:tmdb": "mocha --recursive --grep Tmdb || (exit 0)",
    "documentation": "node ./node_modules/documentation/bin/documentation.js serve --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pochwar/FishBlock.git"
  },
