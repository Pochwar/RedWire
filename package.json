  "devDependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.19.0",
    "lite-server": "^2.3.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "faker": "^4.1.0",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.10.2",
    "multer": "^1.3.0",
    "multer-imager": "^3.1.0",
    "socket.io": "^1.7.3",
    "tmdbapi": "^1.0.2",
    "twig": "^1.10.5",
    "underscore": "^1.8.3",
    "uniqid": "^4.1.1",
    "winston": "^2.3.1"
  },
  "bugs": {
    "url": "https://github.com/Pochwar/FishBlock/issues"
  },
